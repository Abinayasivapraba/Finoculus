var app = angular.module('chatApp',['ngRoute','ngCookies']);
app.config(function($routeProvider) {
	$routeProvider
	.when('/home', {
		templateUrl : 'Header/Home.html'	
	})
	.when('/goRegister', {
		templateUrl : 'User/Register.html',
		controller : 'UserController'
	})
	.when('/login', {
		templateUrl : 'User/Login.html',
		controller : 'UserController'
	})

	.otherwise({
		redirectTo : '/'
			
	});
});